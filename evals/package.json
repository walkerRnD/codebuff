{
  "name": "@codebuff/evals",
  "version": "1.0.0",
  "description": "Evaluation scripts for Codebuff",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "inf": "$(git rev-parse --show-toplevel)/inf",
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test",
    "test:manifold": "bun test manifold.test.ts",
    "test:pglite": "bun test pglite-demo.test.ts",
    "test:swe-bench": "bun test swe-bench.test.ts",
    "test:e2e-cat-app": "bun run e2e-cat-app-script.ts",
    "gen-git-evals": "bun run inf bun run git-evals/gen-evals.ts",
    "run-git-evals": "bun run inf bun run git-evals/run-git-evals.ts",
    "run-eval-set": "bun run inf bun run git-evals/run-eval-set.ts",
    "setup-codebuff-repo": "bun run setup-codebuff-repo.ts"
  },
  "sideEffects": false,
  "engines": {
    "bun": ">=1.2.11"
  },
  "dependencies": {
    "@codebuff/common": "workspace:*",
    "@codebuff/backend": "workspace:*",
    "@codebuff/internal": "workspace:*",
    "@codebuff/code-map": "workspace:*",
    "@codebuff/npm-app": "workspace:*",
    "async": "^3.2.6",
    "lodash": "^4.17.21",
    "p-limit": "^6.2.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.24"
  }
}
