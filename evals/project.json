{
  "name": "evals",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": ".",
  "projectType": "application",
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test",
        "cwd": "evals"
      }
    },
    "test:manifold": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test manifold.test.ts",
        "cwd": "evals"
      }
    },
    "test:pglite": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test pglite-demo.test.ts",
        "cwd": "evals"
      }
    },
    "test:swe-bench": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test swe-bench.test.ts",
        "cwd": "evals"
      }
    },
    "test:e2e-cat-app": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run e2e-cat-app-script.ts",
        "cwd": "evals"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "evals"
      }
    },
    "gen-git-evals": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "bun run git-evals/gen-evals.ts",
        "cwd": "evals"
      }
    },
    "run-git-evals": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "bun run git-evals/run-git-evals.ts",
        "cwd": "evals"
      }
    },
    "run-eval-set": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "bun run evals/git-evals/run-eval-set.ts"
      }
    },
    "setup-codebuff-repo": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run setup-codebuff-repo.ts",
        "cwd": "evals"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["@codebuff/internal", "codebuff"]
}