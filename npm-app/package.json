{
  "name": "manicode",
  "version": "1.0.61",
  "description": "AI dev assistant",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "manicode": "dist/index.js"
  },
  "scripts": {
    "prebuild": "bun clean && cd ../common && bun run build",
    "build": "tsc && tsc-alias",
    "postbuild": "mkdir -p dist/common && cp -r ../common/dist/* dist/common && tsc-alias",
    "clean": "rm -rf dist && (rm tsconfig.tsbuildinfo || true)",
    "prepublishOnly": "bun run build && bun run clean-package.js",
    "postpublish": "echo \"const fs=require('fs');fs.copyFileSync('temp.package.json','package.json');fs.unlinkSync('temp.package.json');\" | bun run -",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "start-js": "bun run dist/index.js ..",
    "start": "ts-node src/index.ts ..",
    "start-manifold": "ts-node src/index.ts ../../manifold",
    "start-litestar": "ts-node src/index.ts ../test/__mock-projects__/litestar",
    "start-jpcsp": "ts-node src/index.ts ../test/__mock-projects__/jpcsp",
    "start-vscode": "ts-node src/index.ts ../test/__mock-projects__/vscode",
    "start-nushell": "ts-node src/index.ts ../test/__mock-projects__/nushell",
    "start-prod": "NODE_ENV=production ts-node src/index.ts",
    "ssh": "gcloud compute ssh manicode-backend --zone=us-east4-a"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@types/diff": "5.2.1",
    "axios": "1.7.4",
    "cheerio": "1.0.0",
    "diff": "5.2.0",
    "ignore": "5.3.2",
    "lodash": "*",
    "nanoid": "5.0.7",
    "picocolors": "1.1.0",
    "systeminformation": "5.23.4",
    "tree-sitter": "0.21.1",
    "tree-sitter-c": "0.23.0",
    "tree-sitter-c-sharp": "0.23.0",
    "tree-sitter-cpp": "0.23.0",
    "tree-sitter-go": "0.23.1",
    "tree-sitter-java": "0.23.2",
    "tree-sitter-javascript": "0.23.0",
    "tree-sitter-php": "0.23.2",
    "tree-sitter-python": "0.23.2",
    "tree-sitter-ruby": "0.23.0",
    "tree-sitter-rust": "0.23.0",
    "tree-sitter-typescript": "0.23.0",
    "ws": "8.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "@types/systeminformation": "^3.54.1",
    "@types/ws": "^8.2.0",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@types/lodash": "*",
    "@types/node": "*",
    "typescript": "*"
  }
}
