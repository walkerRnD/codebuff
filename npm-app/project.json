{
  "name": "npm-app",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist && (rm tsconfig.tsbuildinfo || true)",
        "cwd": "npm-app"
      }
    },
    "build": {
      "outputs": ["{projectRoot}/dist"],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "eslint src/index.ts",
          "tsc",
          "mkdir -p dist/common",
          "cp -r ../common/dist/* dist/common",
          "mkdir -p dist/code-map",
          "cp -r ../packages/code-map/dist/* dist/code-map",
          "tsc-alias -p tsconfig.json",
          "mkdir -p dist/utils/__tests__",
          "cp -r src/utils/__tests__/*.txt dist/utils/__tests__"
        ],
        "cwd": "npm-app",
        "parallel": false
      },
      "dependsOn": ["^build", "root:build-env"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src/**/*.ts",
        "cwd": "npm-app"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun test $(find src -name *.test.ts)",
        "cwd": "npm-app"
      }
    },
    "prepublish": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "bun run clean-package.js",
        "cwd": "npm-app",
        "env": "prod"
      }
    },
    "start": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts ..",
        "cwd": "npm-app"
      }
    },
    "start-js": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "bun run clean-package.js && sh -c 'TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/index.js ..'",
        "cwd": "npm-app"
      }
    },
    "start-max": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts .. --max",
        "cwd": "npm-app"
      }
    },
    "start-lite": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts .. --lite",
        "cwd": "npm-app"
      }
    },
    "start-ask": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts .. --ask",
        "cwd": "npm-app"
      }
    },
    "start-superagent": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts .. --superagent",
        "cwd": "npm-app"
      }
    },
    "start-dir": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts",
        "cwd": "npm-app"
      }
    },
    "start-experimental": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts .. --experimental",
        "cwd": "npm-app"
      }
    },
    "start-manifold": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts ../../manifold",
        "cwd": "npm-app"
      }
    },
    "start-litestar": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts ../test/__mock-projects__/litestar",
        "cwd": "npm-app"
      }
    },
    "start-jpcsp": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts ../test/__mock-projects__/jpcsp",
        "cwd": "npm-app"
      }
    },
    "start-vscode": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts ../test/__mock-projects__/vscode",
        "cwd": "npm-app"
      }
    },
    "start-nushell": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts ../test/__mock-projects__/nushell",
        "cwd": "npm-app"
      }
    },
    "start-prod": {
      "executor": "@codebuff/build-tools:infisical-run",
      "options": {
        "command": "ts-node -r tsconfig-paths/register src/index.ts",
        "cwd": "npm-app"
      }
    }
  },
  "implicitDependencies": ["common", "code-map"]
}