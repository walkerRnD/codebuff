{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend server for Codebuff",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "infisical run --log-level=warn -- bun src/index.ts",
    "dev": "infisical run --log-level=warn -- bun src/index.ts",
    "build": "tsc --build",
    "typecheck-this-package": "tsc --noEmit && tsc-alias",
    "typecheck": "bun run --cwd ../common build && bun run typecheck-this-package",
    "test": "bun test"
  },
  "dependencies": {
    "@ai-sdk/google-vertex": "2.2.24",
    "@ai-sdk/openai": "1.3.22",
    "@anthropic-ai/sdk": "^0.39.0",
    "@codebuff/billing": "workspace:*",
    "@codebuff/internal": "workspace:*",
    "@google-cloud/vertexai": "1.10.0",
    "@google/generative-ai": "0.24.1",
    "ai": "4.3.16",
    "common": "workspace:*",
    "cors": "^2.8.5",
    "diff": "5.2.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "gpt-tokenizer": "2.8.1",
    "ignore": "5.3.2",
    "lodash": "*",
    "openai": "^4.78.1",
    "pino": "9.4.0",
    "postgres": "3.4.4",
    "posthog-node": "^4.14.0",
    "ts-pattern": "5.3.1",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.16",
    "@types/diff": "^5.0.3",
    "@types/express": "^4.17.13",
    "@types/node": "*",
    "@types/ws": "^8.5.5",
    "nodemon": "^2.0.15",
    "pino-pretty": "^11.2.2",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@types/lodash": "*"
  }
}
